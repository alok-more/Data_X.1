<!DOCTYPE html>
<html>
<head>
  <title>Device Settings</title>
  <link rel="stylesheet" type="text/css" href="device_settings.css">
</head>
<body>
  <div class="container">
    <h1>Device Settings</h1>

    <form id="device-settings-form" method="POST" action="/save_device_settings">
  
      <div class="form-group">
        <label for="comPort">COM Port:</label>
        <input type="text" id="comPort" name="comPort" required>
      </div>

      <div class="form-group">
        <label for="baudRate">Baud Rate:</label>
        <input type="text" id="baudRate" name="baudRate" required>
      </div>

      <div class="form-group">
        <label for="parity">Parity:</label>
        <input type="text" id="parity" name="parity" required>
      </div>

      <div class="form-group">
        <label for="stopBits">Stop Bit:</label>
        <input type="text" id="stopBits" name="stopBits" required>
      </div>

      <div class="form-group">
        <label for="dataBits">Data Bit:</label>
        <input type="text" id="dataBits" name="dataBits" required>
      </div>

      <div class="button-container">
        <button type="submit" class="dashboard-button">Save</button>
        <button type="button" class="dashboard-button" onclick="cancel()">Cancel</button>
      </div>
    </form>
  </div>

  <script>
    function cancel() {
      window.location.href = "/";
    }
  </script>
</body>
</html>
